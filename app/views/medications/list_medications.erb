<div class="list-medications">

  <h1>List Medications</h1>

  <% @med.each do |m| %>
    <br><p>Name: <%= m.name %></p>
    <p>Dosage Amount: <%= m.dosage_amount %></p>
    <p>Dosage Form: <%= m.dosage_form %></p>
    <p>Ailment Treated: <%= m.ailment_treated %></p>
  <% end %>

</div>